@{
    Layout="Shared/_Layout.cshtml";
}


@if(Model.Count != 0)
{
    @foreach(var ticket in Model)
    {
        <a href="/ticket/@ticket.GetId()">
            <div class="ticket">
                <p>Ticket Number: @ticket.GetConvertedTicketNum() </p>
                <p>Status: @ticket.GetStatus()</p>
            </div>
        </a>
        <hr/>
    }
}
